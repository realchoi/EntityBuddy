<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="DBuddy.AppUi.Views.HomePage"
             xmlns:vm="using:DBuddy.AppUi.ViewModels"
             x:DataType="vm:HomePageViewModel">
    <StackPanel Margin="10">
        <Grid ShowGridLines="False" Margin="5" ColumnDefinitions="Auto, *, Auto, *"
              RowDefinitions="Auto, Auto, Auto, Auto, Auto, Auto">
            <Button Grid.Row="0" Grid.Column="0" Margin="5 0" Command="{Binding OpenEntityGenerateCommand}">表字段生成实体</Button>
            <Button Grid.Row="0" Grid.Column="1" Margin="5 0">表结构导出为 Excel</Button>
        </Grid>
    </StackPanel>
</UserControl>